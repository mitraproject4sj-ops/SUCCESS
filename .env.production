# Frontend Environment (Netlify)
REACT_APP_BACKEND_URL=https://lakshya-trading-backend.onrender.com
REACT_APP_WS_URL=wss://lakshya-trading-backend.onrender.com

# Trading Configuration
REACT_APP_MAX_TRADES_PER_DAY=15
REACT_APP_DAILY_TARGET=10000
REACT_APP_MAX_DAILY_LOSS=5000
REACT_APP_BASE_RISK_AMOUNT=100
REACT_APP_TOTAL_CAPITAL=50000

# Display Settings
REACT_APP_THEME=dark
REACT_APP_TIMEZONE=Asia/Kolkata

# Feature Flags
REACT_APP_ENABLE_TELEGRAM=true
REACT_APP_ENABLE_EMAIL_REPORTS=true
REACT_APP_ENABLE_GOOGLE_SHEETS=true